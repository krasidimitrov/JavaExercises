CREATE TABLE IF NOT EXISTS People(id INT PRIMARY KEY AUTO_INCREMENT,name VARCHAR(20) NOT NULL, egn CHAR(10) NOT NULL UNIQUE, age INT NOT NULL, email VARCHAR(40) NOT NULL) ENGINE = InnoDB;
CREATE TABLE Trip (
tripID INT PRIMARY KEY AUTO_INCREMENT,
 egn CHAR(10), departureDate DATE,
 returnDate DATE, city VARCHAR(30), FOREIGN KEY (egn) REFERENCES People(egn) ) ENGINE = InnoDB;
